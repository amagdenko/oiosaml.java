<?xml version="1.0" encoding="UTF-8"?>
<XMLTooling xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.opensaml.org/xmltooling-config ../../src/schema/xmltooling-config.xsd"
 xmlns:disco="urn:liberty:disco:2006-08"
 xmlns:sec="urn:liberty:security:2006-08"
 xmlns:sb="urn:liberty:sb:2006-08"
 xmlns:soap12="http://www.w3.org/2003/05/soap-envelope"
 xmlns:wst="http://schemas.xmlsoap.org/ws/2005/02/trust"
 xmlns="http://www.opensaml.org/xmltooling-config">
    
    <ObjectProviders>
    
    	<ObjectProvider qualifiedName="wst:RequestedSecurityToken">
    		<BuilderClass className="org.opensaml.ws.wstrust.impl.RequestedSecurityTokenBuilder" />
    		<MarshallingClass className="org.opensaml.ws.wstrust.impl.RequestedSecurityTokenMarshaller" />
    		<UnmarshallingClass className="org.opensaml.ws.wstrust.impl.RequestedSecurityTokenUnmarshaller" />
    	</ObjectProvider>
    	
    	<ObjectProvider qualifiedName="wst:RequestSecurityTokenResponse">
    		<BuilderClass className="org.opensaml.ws.wstrust.impl.RequestSecurityTokenResponseBuilder" />
    		<MarshallingClass className="org.opensaml.ws.wstrust.impl.RequestSecurityTokenResponseMarshaller" />
    		<UnmarshallingClass className="org.opensaml.ws.wstrust.impl.RequestSecurityTokenResponseUnmarshaller" />
    	</ObjectProvider>

        <ObjectProvider qualifiedName="disco:SecurityMechID">
            <BuilderClass className="dk.itst.oiosaml.liberty.SecurityMechIDBuilder" />
            <MarshallingClass className="dk.itst.oiosaml.liberty.SecurityMechIDMarshaller" />
            <UnmarshallingClass className="dk.itst.oiosaml.liberty.SecurityMechIDUnmarshaller" />
        </ObjectProvider>

        <ObjectProvider qualifiedName="disco:SecurityContext">
            <BuilderClass className="dk.itst.oiosaml.liberty.SecurityContextBuilder" />
            <MarshallingClass className="dk.itst.oiosaml.liberty.SecurityContextMarshaller" />
            <UnmarshallingClass className="dk.itst.oiosaml.liberty.SecurityContextUnmarshaller" />
        </ObjectProvider>        
        <ObjectProvider qualifiedName="sec:Token">
            <BuilderClass className="dk.itst.oiosaml.liberty.Token$Builder" />
            <MarshallingClass className="dk.itst.oiosaml.liberty.Token$Marshaller" />
            <UnmarshallingClass className="dk.itst.oiosaml.liberty.Token$Unmarshaller" />
        </ObjectProvider>
        
        <ObjectProvider qualifiedName="sb:UserInteraction">
        	<BuilderClass className="dk.itst.oiosaml.liberty.UserInteractionBuilder" />
        	<MarshallingClass className="dk.itst.oiosaml.liberty.UserInteractionMarshaller" />
        	<UnmarshallingClass className="dk.itst.oiosaml.liberty.UserInteractionUnmarshaller" />
        </ObjectProvider>
                                
		<ObjectProvider qualifiedName="soap12:Envelope">
        	<BuilderClass className="org.opensaml.ws.soap.soap11.impl.EnvelopeBuilder" />
        	<MarshallingClass className="org.opensaml.ws.soap.soap11.impl.EnvelopeMarshaller" />
        	<UnmarshallingClass className="org.opensaml.ws.soap.soap11.impl.EnvelopeUnmarshaller" />
		</ObjectProvider>
                        
    </ObjectProviders>

</XMLTooling>