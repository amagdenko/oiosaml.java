<?xml version="1.0" encoding="UTF-8"?>
<XMLTooling xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.opensaml.org/xmltooling-config ../../src/schema/xmltooling-config.xsd"
 xmlns:wst="http://docs.oasis-open.org/ws-sx/ws-trust/200512/"
 xmlns:wssu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd"
 xmlns="http://www.opensaml.org/xmltooling-config">
    
    <!--
    	Objects from Liberty ID-WSF Security Mechanisms Specification XSD
    -->
    
    <ObjectProviders>

        <ObjectProvider qualifiedName="wst:RequestSecurityToken">
            <BuilderClass className="dk.itst.oiosaml.trust.RequestSecurityTokenBuilder" />
            <MarshallingClass className="dk.itst.oiosaml.trust.TrustObjectMarshaller" />
            <UnmarshallingClass className="dk.itst.oiosaml.trust.RequestSecurityTokenUnmarshaller" />
        </ObjectProvider>
        
        <ObjectProvider qualifiedName="wst:RequestSecurityTokenResponseCollection">
            <BuilderClass className="dk.itst.oiosaml.trust.RequestSecurityTokenResponseCollectionBuilder" />
            <MarshallingClass className="dk.itst.oiosaml.trust.TrustObjectMarshaller" />
            <UnmarshallingClass className="dk.itst.oiosaml.trust.RequestSecurityTokenResponseCollectionUnmarshaller" />
        </ObjectProvider>
        
        <ObjectProvider qualifiedName="wst:RequestSecurityTokenResponse">
            <BuilderClass className="dk.itst.oiosaml.trust.RequestSecurityTokenResponseBuilder" />
            <MarshallingClass className="dk.itst.oiosaml.trust.TrustObjectMarshaller" />
            <UnmarshallingClass className="dk.itst.oiosaml.trust.RequestSecurityTokenResponseUnmarshaller" />
        </ObjectProvider>

        <ObjectProvider qualifiedName="wst:RequestedSecurityToken">
            <BuilderClass className="dk.itst.oiosaml.trust.RequestedSecurityTokenBuilder" />
            <MarshallingClass className="dk.itst.oiosaml.trust.TrustObjectMarshaller" />
            <UnmarshallingClass className="dk.itst.oiosaml.trust.RequestedSecurityTokenUnmarshaller" />
        </ObjectProvider>
        
        <ObjectProvider qualifiedName="wssu:SecurityTokenReference">
            <BuilderClass className="dk.itst.oiosaml.trust.SecurityTokenReferenceBuilder" />
            <MarshallingClass className="dk.itst.oiosaml.trust.TrustObjectMarshaller" />
            <UnmarshallingClass className="dk.itst.oiosaml.trust.SecurityTokenReferenceUnmarshaller" />
        </ObjectProvider>
        
        <ObjectProvider qualifiedName="wssu:KeyIdentifier">
            <BuilderClass className="dk.itst.oiosaml.trust.KeyIdentifierBuilder" />
            <MarshallingClass className="org.opensaml.xml.schema.impl.XSAnyMarshaller" />
            <UnmarshallingClass className="org.opensaml.xml.schema.impl.XSAnyUnmarshaller" />
        </ObjectProvider>
    </ObjectProviders>
    
</XMLTooling>