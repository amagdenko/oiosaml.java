<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>dk.itst.idws.sts.TokenService</servlet-name>
        <servlet-class>dk.itst.idws.sts.TokenService</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>com.sun.xml.ws.mex.server.MEXEndpoint</servlet-name>
        <servlet-class>com.sun.xml.ws.mex.server.MEXEndpoint</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>dk.itst.idws.sts.TokenService</servlet-name>
        <url-pattern>/TokenServiceService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>com.sun.xml.ws.mex.server.MEXEndpoint</servlet-name>
        <url-pattern>/TokenServiceService/mex</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
